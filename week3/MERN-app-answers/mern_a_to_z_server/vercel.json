{
    "builds": [
        {
          "src": "app.js",
          "use": "@vercel/node"
        },
        {
          "src": "routes/api/books.js",
          "use": "@vercel/node"
        },
        {
          "src": "config/db.js",
          "use": "@vercel/node"
        },
        {
            "src": "config/default.json",
            "use": "static"
          }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js" 
      }
    ]
  }