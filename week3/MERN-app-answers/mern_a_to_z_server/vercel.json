{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node"
      },
      {
        "src": "/routes/api/books.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/routes/api/books.js",
        "use": "@now/node"
      }
    ]
  }